{
  "name": "infinitris2-app",
  "private": true,
  "version": "0.1.0",
  "description": "",
  "dependencies": {
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@hookform/resolvers": "^2.8.5",
    "@mui/icons-material": "^5.2.1",
    "@mui/lab": "^5.0.0-alpha.59",
    "@mui/material": "^5.2.3",
    "babel-loader": "8.1.0",
    "babel-plugin-react-intl": "^8.2.22",
    "bolt11": "^1.4.0",
    "customize-cra": "^1.0.0",
    "date-fns": "^2.28.0",
    "deep-object-diff": "^1.1.0",
    "firebase": "^9.4.1",
    "howler": "^2.2.3",
    "infinitris2-models": "^0.1",
    "json-stable-stringify": "^1.0.1",
    "lodash.merge": "^4.6.2",
    "lottie-react": "^2.1.0",
    "notistack": "^2.0.5",
    "react": "^17.0.2",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.22.5",
    "react-intersection-observer": "^8.32.2",
    "react-intl": "^5.10.9",
    "react-loader-spinner": "^3.1.14",
    "react-qr-code": "^2.0.2",
    "react-router-dom": "^5.2.0",
    "react-script-tag": "^1.1.2",
    "react-scripts": "^4.0.3",
    "react-star-rating-component": "^1.4.1",
    "react-swipeable-views": "^0.14.0",
    "react-use": "^15.3.4",
    "smoothscroll-polyfill": "^0.4.4",
    "swr": "^1.3.0",
    "swr-firestore": "^1.0.0",
    "ts-debounce": "^4.0.0",
    "typescript": "^4.3",
    "uuid": "^8.3.2",
    "webpack": "4.44.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "3.11.1",
    "yup": "^0.32.11",
    "zustand": "^3.1.3"
  },
  "scripts": {
    "start": "REACT_APP_VERSION=$(git rev-list HEAD --count) dotenv -e .env react-app-rewired start",
    "build": "REACT_APP_VERSION=$(git rev-list HEAD --count) dotenv -e .env.prod react-app-rewired build",
    "build-local": "REACT_APP_VERSION=$(git rev-list HEAD --count) dotenv -e .env.local react-app-rewired build",
    "serve": "cd build && python3 -m http.server 3000",
    "deploy": "(cd .. && yarn firebase:deploy:hosting)",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "formatjs:extract": "formatjs extract 'src/**/*.tsx' --out-file src/internationalization/lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "formatjs:compile:en": "./formatjs-compile.sh en",
    "formatjs:compile:th": "./formatjs-compile.sh th",
    "formatjs:compile:all": "yarn formatjs:compile:en && yarn formatjs:compile:th",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lt": "lt --port 3000 --subdomain infinitris",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "unused-imports"
    ],
    "rules": {
      "no-sparse-arrays": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "react-hooks/exhaustive-deps": "error",
      "unused-imports/no-unused-imports": "error",
      "unused-imports/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rolznz/infinitris2.git"
  },
  "keywords": [
    "infinitris2",
    "app"
  ],
  "author": "rolznz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rolznz/infinitris2/issues"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "^2.13.16",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.3.10",
    "@storybook/addon-docs": "^6.3.10",
    "@storybook/addon-essentials": "^6.3.10",
    "@storybook/addon-links": "^6.3.10",
    "@storybook/addons": "^6.3.10",
    "@storybook/node-logger": "^6.3.10",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.3.10",
    "@storybook/theming": "^6.3.10",
    "@types/howler": "^2.2.3",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.4",
    "@types/react-loader-spinner": "^3.1.2",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-star-rating-component": "^1.4.0",
    "@types/react-swipeable-views": "^0.13.1",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/uuid": "^8.3.0",
    "dotenv-cli": "^5.0.0",
    "eslint": "^7.6.0",
    "eslint-plugin-unused-imports": "^2.0.0"
  }
}
