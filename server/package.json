{
  "name": "infinitris2-server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clean": "rm -rf dist && find src spec -name \"*.js\" -o -name \"*.d.*\" -type f -delete",
    "test": "tsc --build && ts-node node_modules/jasmine/bin/jasmine --config=spec/support/jasmine.json",
    "coverage": "nyc --cache false -r lcov -r text npm run test",
    "build": "webpack",
    "watch": "webpack --watch",
    "start": "node -r source-map-support/register dist/bundle.js",
    "debug": "node --nolazy --inspect-brk dist/bundle.js",
    "docs": "typedoc --out docs --readme README.md src",
    "tslint": "tslint --project ./tsconfig.json"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "spec/**/*",
      "coverage/**/*",
      "dist/**/*",
      "docs/**/*",
      "webpack.config.js"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rolznz/infinitris2.git"
  },
  "keywords": [
    "infinitris2",
    "server"
  ],
  "author": "rolznz <roland.bewick@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rolznz/infinitris2/issues"
  },
  "homepage": "https://github.com/rolznz/infinitris2#readme",
  "devDependencies": {
    "@types/jasmine": "^3.4.0",
    "@types/node": "^12.7.8",
    "@types/ws": "^6.0.3",
    "awesome-typescript-loader": "^5.2.1",
    "jasmine": "^3.5.0",
    "module-alias": "^2.2.1",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  },
  "dependencies": {
    "ws": "^7.1.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  },
  "_moduleAliases": {
    "@core": "../core/src",
    "@src": "src"
  }
}
