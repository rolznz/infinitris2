{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [
            true,
            "always"
        ],
        "eofline": false,
        "quotemark": [
            true,
            "double",
            "avoid-escape"
        ],
        "one-line": false,
        "arrow-parens": false,
        "ordered-imports": false,
        "variable-name": {
            "options": [
                "ban-keywords",
                "check-format",
                "allow-leading-underscore"
            ]
        },
        "no-console": false,
        "member-access": false
    },
    "rulesDirectory": []
}